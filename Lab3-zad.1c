// Napisz program który pobierze od użytkownika dwie liczby i zwróci wynik ich:
//dodawaia
//odejmowania
//mnożenia
//dzielenia
//resztę z dzielenia

#include <stdio.h>
int main(){
int a, b;

printf("Podaj dwie liczby: ");
scanf("%d %d", &a, &b);

printf("Dodawanie: %d\n ", a + b);
printf("Odejmowanie: %d\n", a - b);
printf("Mnozenie: %d\n", a * b);


if(b != 0)
    printf("Dzielnie: %d\n", a / b);
else { printf("Nie mozna dzielic przez 0\n");}
return 0;

}
